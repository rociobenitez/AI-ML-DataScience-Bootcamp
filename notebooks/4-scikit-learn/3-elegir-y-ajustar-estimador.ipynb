    "# Importar la clase del modelo RandomForestRegressor del m칩dulo ensemble\n",
    "from sklearn.ensemble import RandomForestRegressor\n",
    "\n",
    "# Configurar la semilla aleatoria\n",
    "np.random.seed(42)\n",
    "\n",
    "# Crear los datos\n",
    "X = housing_df.drop(\"target\", axis=1)\n",
